{
  "name": "rainier-dynamics-ops-engine",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev:lab",
    "dev:lab": "npm run dev -w @rainier-dynamics/lab",
    "build": "npm run build:lab",
    "build:lab": "npm run build -w @rainier-dynamics/lab",
    "start": "npm run start:lab",
    "start:lab": "npm run start -w @rainier-dynamics/lab",
    "lint": "npm run lint --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "typecheck": "npm run typecheck --workspaces --if-present"
  }
}
